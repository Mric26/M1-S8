package modele;

import java.util.Vector;

public class Caddie {

	private int noCaddie;
	private double montant;
	private Vector<Representation> representations = new Vector<Representation>();
	
	public Caddie(){
		setNoCaddie((int) (Math.random()*9999));
		representations = new Vector<Representation>();
		setMontant(0);
	}

	public int getNoCaddie() {
		return noCaddie;
	}

	public void setNoCaddie(int noCaddie) {
		this.noCaddie = noCaddie;
	}

	public double getMontant() {
		return montant;
	}

	public void setMontant(double montant) {
		this.montant = montant;
	}
	
}
